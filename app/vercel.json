{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python@3.1.49"
        }
    ],
    "routes": [
        {
            "src": "/analyze",
            "dest": "app.py"
        },
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ]
}
